<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.proyecto.mapper.LenguajeMapper">
	
	<select id="getLenguajes" resultMap="lenguajes">
		  SELECT 
	          id
	       FROM lenguajes
	</select>
	
	<resultMap id="lenguajes" type="com.proyecto.modelo.Lenguaje">
			<id property="id" column="id" javaType="java.lang.String"/>
	</resultMap>
</mapper>